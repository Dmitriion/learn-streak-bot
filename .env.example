
# Telegram Mini App Configuration
# Скопируйте этот файл в .env.local и заполните значения

# N8N Webhook URL для интеграции автоматизации
# Получите URL в вашем N8N instance: Workflow -> Settings -> Webhook URL
VITE_N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/telegram-edu-app

# Telegram Bot Token (только для development и тестирования)
# Получите токен у @BotFather в Telegram
VITE_TELEGRAM_BOT_TOKEN=your_bot_token_here

# Production Environment (автоматически устанавливается при сборке)
VITE_APP_ENV=production

# Дополнительные настройки для production
VITE_ENABLE_MOCK_MODE=false
VITE_LOG_LEVEL=info
